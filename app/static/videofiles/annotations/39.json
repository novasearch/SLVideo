{
  "LP_P1 transcri\u00e7\u00e3o livre": {
    "linguistic_type_ref": "default-lt",
    "tier_id": "LP_P1 transcri\u00e7\u00e3o livre",
    "annotations": [
      {
        "annotation_id": "a151",
        "start_time": "2",
        "end_time": "1460",
        "value": "O tema \u00e9 sobre Braidwood"
      },
      {
        "annotation_id": "a152",
        "start_time": "1871",
        "end_time": "5711",
        "value": "Foi influenciado pelo m\u00e9todo do Wallis"
      },
      {
        "annotation_id": "a153",
        "start_time": "6430",
        "end_time": "8719",
        "value": "J\u00e1 abriu a escola na Esc\u00f3cia!"
      },
      {
        "annotation_id": "a154",
        "start_time": "8802",
        "end_time": "10808",
        "value": "O m\u00e9todo dele implica gestuar"
      },
      {
        "annotation_id": "a157",
        "start_time": "11132",
        "end_time": "12845",
        "value": "Ensinava a escrever palavras"
      },
      {
        "annotation_id": "a159",
        "start_time": "12943",
        "end_time": "14744",
        "value": "O m\u00e9todo dele foi mantido em segredo"
      },
      {
        "annotation_id": "a168",
        "start_time": "15495",
        "end_time": "19034",
        "value": "Ao longo dos anos at\u00e9 ao momento, a fam\u00edlia Braidwood j\u00e1 percebeu"
      },
      {
        "annotation_id": "a160",
        "start_time": "19203",
        "end_time": "22465",
        "value": " que os surdos n\u00e3o precisam aprender a oralizar"
      },
      {
        "annotation_id": "a161",
        "start_time": "24295",
        "end_time": "25246",
        "value": "Ai!"
      }
    ],
    "parent_ref": null
  },
